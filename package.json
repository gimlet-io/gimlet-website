{
  "name": "eleventy-webpack-tailwind",
  "description": "Eleventy project with Webpack and Tailwind CSS",
  "version": "1.0.0",
  "author": "Code Cookbook",
  "license": "MIT",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:assets": "webpack --watch",
    "dev:site": "eleventy --serve",
    "build": "run-s clean build:assets build:site",
    "build:assets": "NODE_ENV=production webpack --mode production",
    "build:site": "NODE_ENV=production eleventy",
    "clean": "rm -rf ./dist"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.4",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "luxon": "^1.25.0",
    "markdown-it-anchor": "^6.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.8",
    "postcss-loader": "^4.1.0",
    "tailwindcss": "^2.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.3.1",
    "alpinejs": "^2.7.3",
    "markdown-it": "^12.2.0"
  }
}
