<!doctype html>
<html>
<head>
    <title>{% if title %}{{ title }} | {% endif %}{{ site.title }}</title>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="{% if description %}{{ description }}{% else %}{{ site.description }}{% endif %}">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <meta content="{% if title %}{{ title }} | {% endif %}{{ site.title }}" property="og:title">
    <meta content="website" property="og:type">
    <meta content="{{ 'https://gimlet.io/'+image | default('gimlet.png') }}" property="og:image">
    <meta content="{{ site.url }}{{ page.url }}" property="og:url">
    <meta content="Gimlet" property="og:site_name">
    <meta content="{{ excerpt | default('Readymade automation for the common Kubernetes deployment workflows. If you are hiring your first DevOps, or you need a headstart') }}"
          property="og:description">

    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="@laszlocph">
    <meta name="twitter:creator" content="@laszlocph">
    <meta name="twitter:title" content="{% if title %}{{ title }} | {% endif %}{{ site.title }}">
    <meta name="twitter:description"
          content="{{ excerpt | default('Readymade automation for the common Kubernetes deployment workflows. If you are hiring your first DevOps, or you need a headstart') }}">
    <meta name="twitter:image" content="{{ 'https://gimlet.io/'+image | default('gimlet.png') }}">

    <link rel="canonical" href="{{ site.url }}{{ page.url }}">
    <link rel="stylesheet" href="https://rsms.me/inter/inter.css">
    {% bundledCss %}

</head>
