<!doctype html>
<html>
<head>
    <title>{% if title %}{{ title }} | {% endif %}{{ site.title }}</title>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="{% if description %}{{ description }}{% else %}{{ site.description }}{% endif %}">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <meta content="{% if title %}{{ title }} | {% endif %}{{ site.title }}" property="og:title">
    <meta content="website" property="og:type">
    <meta content="{{ 'https://gimlet.io/'+image | default('logo-social2.png') }}" property="og:image">
    <meta content="{{ site.url }}{{ page.url }}" property="og:url">
    <meta content="Gimlet" property="og:site_name">
    <meta content="{{ excerpt | default('A modular GitOps workflow for Kubernetes deployments that grows with you. It\'s open-source.') }}"
          property="og:description">

    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:site" content="@laszlocph">
    <meta name="twitter:creator" content="@laszlocph">
    <meta name="twitter:title" content="{% if title %}{{ title }} | {% endif %}{{ site.title }}">
    <meta name="twitter:description"
          content="{{ excerpt | default('A modular GitOps workflow for Kubernetes deployments that grows with you. It\'s open-source.') }}">
    <meta name="twitter:image" content="{{ 'https://gimlet.io/'+image | default('logo-social2.png') }}">

    <link rel="canonical" href="{{ site.url }}{{ page.url }}">
    <link rel="stylesheet" href="https://rsms.me/inter/inter.css">
    {% bundledCss %}

</head>
<body>
{% bundledJs %}

<div class="bg-gray-900 text-white">
    <div class="max-w-7xl py-2 px-4 sm:px-6 mx-auto">
        Gimlet is on the FOSDEM
        <a class="text-green-400" href="https://fosdem.org/2021/schedule/event/stairstep_your_kubernetes_deployment_workflow_with_gimlet_and_gitops/">
            conference schedule
        </a>
    </div>
</div>
<div class="relative bg-white overflow-hidden">
